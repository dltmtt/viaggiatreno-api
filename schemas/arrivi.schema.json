            },
            "compImgRitardo": {
              "const": "/vt_static/img/legenda/icone_legenda/regolare.png"
            }
          }
        },
        "else": {
          "$comment": "Se il treno NON è stato cancellato completamente.",
          "properties": {
            "millisDataPartenza": {
              "const": null
            }
          }
        }
      },
      {
        "if": {
          "$comment": "Se il treno è riprogrammato.",
          "properties": {
            "riprogrammazione": {
              "const": "Y"
            }
