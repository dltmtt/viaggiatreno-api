    "allOf": [
      {
        "if": {
          "$comment": "Se il treno è stato cancellato completamente.",
          "properties": {
            "provvedimento": {
              "const": 1
            }
          }
        },
        "then": {
          "properties": {
            "millisDataPartenza": {
              "type": "string"
            },
            "riprogrammazione": {
              "const": null
            },
            "compRitardo": {
              "const": [
                "Cancellato",
                "Canceled",
                "Storniert",
                "Annul&eacute",
                "Cancelado",
                null,
                null,
                null,
                null
