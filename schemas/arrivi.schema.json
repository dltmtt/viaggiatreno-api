        "enum": ["regionale", "nazionale"],
        "description": "Tipologia del treno."
      },
      "compClassRitardoTxt": {
        "enum": ["", "ritardo01_txt", "ritardo02_txt", "ritardo03_txt", null]
      },
      "compClassRitardoLine": {
        "enum": [
          "regolare_line",
          "ritardo01_line",
          "ritardo02_line",
          "ritardo03_line",
